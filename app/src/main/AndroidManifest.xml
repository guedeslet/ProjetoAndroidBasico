<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.projetoandroidbsico">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/projeto"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:usesCleartextTraffic="true"
        android:theme="@style/AppTheme">


        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="@string/google_maps_key" />


        <activity android:name=".ActivityMaps"
            android:parentActivityName=".ActivityList">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".ActivityList" />
        </activity>


        <activity
            android:name=".ActivityNews"
            android:parentActivityName=".ActivityList">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".ActivityList" />
        </activity>


        <activity android:name=".ActivityList">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".ActivityRegister" />
        </activity>


        <activity
            android:name=".ActivityRegister"
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>


        <activity
            android:name=".ActivityLogin"
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>


        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>


        <service
            android:name=".MyJobService"
            android:permission="android.permission.BIND_JOB_SERVICE" />

    </application>

</manifest>